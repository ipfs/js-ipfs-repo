    "ipfs-repo-migrations": "^9.0.0",
    "it-filter": "^1.0.2",
    "it-map": "^1.0.5",
    "it-merge": "^1.0.2",
    "it-parallel-batch": "^1.0.9",
    "it-pipe": "^1.1.0",
    "it-pushable": "^1.4.0",
    "just-safe-get": "^2.0.0",
    "just-safe-set": "^2.1.0",
    "merge-options": "^3.0.4",
    "mortice": "^2.0.1",
    "multiformats": "^9.0.4",
    "p-queue": "^6.0.0",
    "proper-lockfile": "^4.0.0",
