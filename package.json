    "events": "^3.3.0",
    "ipfs-utils": "^8.1.3",
    "it-all": "^1.0.2",
    "it-drain": "^1.0.1",
    "it-first": "^1.0.2",
    "just-range": "^2.1.0",
    "rimraf": "^3.0.0",
    "sinon": "^11.1.1",
    "tsd": "^0.17.0",
    "url": "^0.11.0",
    "util": "^0.12.3"
  },
  "dependencies": {
